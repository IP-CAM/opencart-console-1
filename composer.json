{
    "name": "rmanara/console",
    "description": "app to manage OC extension, data or modules also build extension or module for opencart",
    "license": "MIT", 
    "version": "1.0.0",  
    "authors": [
        {
            "name": "Rodrigo Manara",
            "email": "me@rodrigomanara.co.uk",
            "homepage": "http://rodrigomanara.co.uk"
        }
    ],
    "require": {
        "symfony/yaml": "^3.3",
        "symfony/console": "^3.3",
        "symfony/finder": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Rmanara\\App\\": "src/App/",
            "Rmanara\\Lib\\": "src/Lib/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
 